{
  "bind_addr": "0.0.0.0",
  "bootstrap_expect": {{ docker_consul_server_bootstrap_expect }},
  "server": true,
  "ui": true,
  "acl_default_policy": "deny",
  "http_config": {
    "response_headers": {
      "Access-Control-Allow-Origin": "*"
    }
  },
  "rejoin_after_leave": true,
  "leave_on_terminate": false,
  "disable_update_check": true,
  "retry_join": {{ docker_consul_server_retry_join_json }},
  "retry_join_wan": {{ docker_consul_server_retry_join_wan_json }},
  "recursors": {{ docker_consul_server_recursors_json }},
  "advertise_addr": "{{ docker_consul_server_advertise_addr }}",
  "advertise_addr_wan": "{{ docker_consul_server_advertise_addr_wan }}",
  "datacenter": "{{ docker_consul_server_datacenter }}",
  "acl_datacenter": "{{ docker_consul_server_acl_datacenter }}",
  "acl_token": "{{ docker_consul_server_acl_token }}",
  "acl_master_token": "{{ docker_consul_server_acl_master_token }}",
  "acl_agent_master_token": "{{ docker_consul_server_acl_agent_master_token }}",
  "encrypt": "{{ docker_consul_server_encrypt }}"
}
